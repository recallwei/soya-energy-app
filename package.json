{
  "name": "soya-energy-app",
  "author": "Bruce Song <wenwei.song@raipiot.com>",
  "scripts": {
    "start": "react-native start --reset-cache --experimental-debugger",
    "clean": "react-native clean",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:aab:staging": "yarn android:clean && react-native build-android --mode=staging && open android/app/build/outputs/bundle/staging",
    "android:aab:prod": "yarn android:clean && react-native build-android --mode=release && open android/app/build/outputs/bundle/release",
    "android:apk:staging": "yarn android:clean && cd android && ./gradlew assembleStaging && cd .. && open android/app/build/outputs/apk/staging",
    "android:apk:prod": "yarn android:clean && cd android && ./gradlew assembleRelease && cd .. && open android/app/build/outputs/apk/release",
    "ios:cp:staging": "appcenter codepush release-react -a soya-energy/soya-energy-ios -d Staging -m false -p ios/SoyaEnergyApp/Info.plist --xcode-project-file=ios/SoyaEnergyApp.xcodeproj --sourcemap-output-dir ./build --output-dir ./build",
    "ios:cp:prod": "appcenter codepush release-react -a soya-energy/soya-energy-ios -d Production -x true -m false -p ios/SoyaEnergyApp/Info.plist --xcode-project-file=ios/SoyaEnergyApp.xcodeproj --sourcemap-output-dir ./build --output-dir ./build",
    "android:cp:staging": "appcenter codepush release-react -a soya-energy/soya-energy-android -d Staging -m false --sourcemap-output-dir ./build --output-dir ./build",
    "android:cp:prod": "appcenter codepush release-react -a soya-energy/soya-energy-android -d Production -x true -m false --sourcemap-output-dir ./build --output-dir ./build",
    "cp:staging": "yarn ios:cp:staging && yarn android:cp:staging",
    "cp:prod": "yarn ios:cp:prod && yarn android:cp:prod",
    "cp:list": "appcenter codepush deployment list -a soya-energy/soya-energy-ios && appcenter codepush deployment list -a soya-energy/soya-energy-android",
    "cp:clean": "appcenter codepush deployment clear -a soya-energy/soya-energy-ios Staging && appcenter codepush deployment clear -a soya-energy/soya-energy-android Staging",
    "doctor": "react-native doctor",
    "devtool": "npx react-devtools",
    "pod:install": "npx pod-install",
    "assets:install": "npx react-native-asset",
    "cspell:check": "cspell \"**\"",
    "eslint:check": "eslint \"**/*.{ts,tsx,js,jsx}\" --color",
    "eslint:fix": "yarn eslint:check --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "test": "jest",
    "cz": "yarn git-cz",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@baronha/ting": "^1.1.0",
    "@georstat/react-native-image-cache": "^3.1.0",
    "@hookform/resolvers": "^3.3.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/cli-platform-android": "^13.0.0",
    "@react-native-community/geolocation": "^3.1.0",
    "@react-native-community/netinfo": "^11.2.0",
    "@react-native/gradle-plugin": "^0.73.4",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@sentry/react-native": "^5.15.1",
    "@tamagui/babel-plugin": "^1.79.9",
    "@tamagui/config": "^1.79.9",
    "@tamagui/lucide-icons": "^1.79.9",
    "@tanstack/react-query": "^5.12.2",
    "ahooks": "^3.7.8",
    "appcenter": "^5.0.0",
    "appcenter-analytics": "^5.0.0",
    "appcenter-crashes": "^5.0.0",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "i18next": "^23.7.8",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
    "react-native": "0.73.0",
    "react-native-blob-util": "^0.19.6",
    "react-native-code-push": "^8.1.0",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-drawer-layout": "^3.2.2",
    "react-native-file-access": "^3.0.4",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-gifted-charts": "^1.3.24",
    "react-native-image-crop-picker": "^0.40.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-maps": "^1.8.3",
    "react-native-pdf": "^6.7.3",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-vision-camera": "3.6.13",
    "react-native-webview": "^13.6.3",
    "tamagui": "^1.79.9",
    "use-immer": "^0.9.0",
    "yup": "^1.3.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@brucesong/eslint-config-react-native": "^0.0.16",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@react-native/babel-preset": "^0.74.0",
    "@react-native/metro-config": "^0.74.0",
    "@react-native/typescript-config": "^0.74.0",
    "@types/crypto-js": "^4.2.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.42",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.3.0",
    "cspell": "^8.1.3",
    "cz-git": "^1.8.0",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "lint-staged": "^15.2.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "private": true
}
